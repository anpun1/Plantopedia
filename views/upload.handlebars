<div class="jumbotron jumbotron-fluid">
  <h1 class="display-4" style="text-align:center"> Are you uploading a leaf or flower?</h1>
</div>

<div class="main-content-box">
  <div class="main-content">
    <div class="row">
      <div class="img-5 column"></div>
      <div class="img-6 column"> </div>
    </div>

    <form>
      <div class="row">
        <div class="column-text">
          <input class="form-check-input" type="radio" name="Radio" id="flowerRadio" value="option1" checked>
          <label class="radio">
            Leaf
          </label>
        </div>
        <div class="column-text">
          <input class="form-check-input" type="radio" name="Radio" id="leafRadio" value="option2">
          <label class="radio">
            Flower
          </label>
        </div>
      </div>
      <div class='column-text'>
        <input type="file" id="myFile" name="filename">
      </div>
      <div class='column-text'>
        <input type="submit" id="submit-btn" />
      </div>
    </form>

    <div id="plantid">
    </div>
    <script src="/upload.js"></script>
  </div>
</div>